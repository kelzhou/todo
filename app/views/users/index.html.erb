<h1>Users List</h1>

<table>
 	<tr>
 		<td>Name</td>
 		<td>Email</td>
 		<td>Password</td>
 	</tr>
 	<% @users.each do |user| %>
 	<tr>
 		<td><%= user.name %></td>
 		<td><%= user.email %></td>
 		<td><%= user.password %></td>
 	</tr>
 	<% end %>
</table>

<%= link_to "New User", new_user_path %>

